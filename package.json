{
  "name": "otus-react-storybook",
  "description": "React Storybook",
  "keywords": [
    "typescript",
    "react",
    "storybook"
  ],
  "homepage": "https://github.com/alexey-sidorov-dev/otus-react-storybook#readme",
  "bugs": {
    "url": "https://github.com/alexey-sidorov-dev/otus-react-storybook/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alexey-sidorov-dev/otus-react-storybook.git"
  },
  "license": "ISC",
  "author": "Alexey Sidorov",
  "main": "./src/index.tsx",
  "scripts": {
    "build": "webpack build --define-process-env-node-env production",
    "build:analyze": "webpack build --define-process-env-node-env production && webpack --analyze",
    "dev": "webpack serve --define-process-env-node-env development --open",
    "lint": "eslint . --ext .js,.ts,.tsx && tsc --noEmit",
    "lint:all": "eslint . --ext .js,.ts,.tsx && tsc --noEmit && stylelint **/*.{css,scss} && prettier . --check",
    "lint:all:fix": "eslint . --ext .js,.ts,.tsx --fix && tsc --noEmit && stylelint **/*.{css,scss} --fix && prettier . --write",
    "lint:fix": "eslint . --ext .js,.ts,.tsx --fix",
    "ncu": "npm-check-updates --interactive --format group --doctor",
    "prepare": "husky install",
    "prettier": "prettier . --check",
    "prettier:fix": "prettier . --write",
    "storybook": "start-storybook -p 6006",
    "storybook:build": "build-storybook",
    "stylelint": "stylelint **/*.{css,scss}",
    "stylelint:fix": "stylelint **/*.{css,scss} --fix",
    "test": "jest",
    "test:coverage": "jest --coverage"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.21.0",
    "@babel/core": "^7.21.3",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.21.0",
    "@storybook/addon-actions": "^6.5.16",
    "@storybook/addon-essentials": "^6.5.16",
    "@storybook/addon-interactions": "^6.5.16",
    "@storybook/addon-links": "^6.5.16",
    "@storybook/builder-webpack5": "^6.5.16",
    "@storybook/manager-webpack5": "^6.5.16",
    "@storybook/react": "^6.5.16",
    "@storybook/testing-library": "^0.0.13",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/config": "^3.3.0",
    "@types/eslint": "^8.21.3",
    "@types/jest": "^29.5.0",
    "@types/jsdom": "^21.1.1",
    "@types/node": "^18.15.9",
    "@types/prettier": "^2.7.2",
    "@types/react": "^18.0.29",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.56.0",
    "@typescript-eslint/parser": "^5.56.0",
    "babel-loader": "^9.1.2",
    "chromatic": "^6.17.2",
    "copy-webpack-plugin": "^11.0.0",
    "core-js": "^3.29.1",
    "css-loader": "^6.7.3",
    "css-minimizer-webpack-plugin": "^4.2.2",
    "eslint": "^8.36.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-storybook": "^0.6.11",
    "html-loader": "^4.2.0",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^8.0.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "lint-staged": "^13.2.0",
    "mini-css-extract-plugin": "^2.7.5",
    "npm-check-updates": "^16.8.0",
    "prettier": "^2.8.7",
    "prettier-plugin-packagejson": "^2.4.3",
    "prettier-plugin-sh": "^0.12.8",
    "sass": "^1.60.0",
    "sass-loader": "^13.2.1",
    "style-loader": "^3.3.2",
    "stylelint": "^15.3.0",
    "stylelint-config-standard-scss": "^7.0.1",
    "ts-jest": "^29.0.5",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5",
    "webpack": "^5.76.3",
    "webpack-bundle-analyzer": "^4.8.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^4.13.1"
  }
}
